#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/philipschonholzer/Development/react-schulung/react-schulung/node_modules/.pnpm/@slidev+cli@0.43.7_@types+markdown-it@13.0.2_postcss@8.4.31/node_modules/@slidev/cli/bin/node_modules:/Users/philipschonholzer/Development/react-schulung/react-schulung/node_modules/.pnpm/@slidev+cli@0.43.7_@types+markdown-it@13.0.2_postcss@8.4.31/node_modules/@slidev/cli/node_modules:/Users/philipschonholzer/Development/react-schulung/react-schulung/node_modules/.pnpm/@slidev+cli@0.43.7_@types+markdown-it@13.0.2_postcss@8.4.31/node_modules/@slidev/node_modules:/Users/philipschonholzer/Development/react-schulung/react-schulung/node_modules/.pnpm/@slidev+cli@0.43.7_@types+markdown-it@13.0.2_postcss@8.4.31/node_modules:/Users/philipschonholzer/Development/react-schulung/react-schulung/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/philipschonholzer/Development/react-schulung/react-schulung/node_modules/.pnpm/@slidev+cli@0.43.7_@types+markdown-it@13.0.2_postcss@8.4.31/node_modules/@slidev/cli/bin/node_modules:/Users/philipschonholzer/Development/react-schulung/react-schulung/node_modules/.pnpm/@slidev+cli@0.43.7_@types+markdown-it@13.0.2_postcss@8.4.31/node_modules/@slidev/cli/node_modules:/Users/philipschonholzer/Development/react-schulung/react-schulung/node_modules/.pnpm/@slidev+cli@0.43.7_@types+markdown-it@13.0.2_postcss@8.4.31/node_modules/@slidev/node_modules:/Users/philipschonholzer/Development/react-schulung/react-schulung/node_modules/.pnpm/@slidev+cli@0.43.7_@types+markdown-it@13.0.2_postcss@8.4.31/node_modules:/Users/philipschonholzer/Development/react-schulung/react-schulung/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@slidev/cli/bin/slidev.js" "$@"
else
  exec node  "$basedir/../@slidev/cli/bin/slidev.js" "$@"
fi
